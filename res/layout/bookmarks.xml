<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="@color/white">
	<RelativeLayout android:id="@+id/widget86"
		android:layout_width="fill_parent" android:layout_height="29dip" android:background="@color/random">
		<ImageView android:id="@+id/imageView1"
			android:layout_alignParentLeft="true" android:layout_width="20dip" android:layout_marginLeft="5dip" android:background="@drawable/bookmarks" android:layout_height="20dip" android:layout_marginTop="4dip"></ImageView>
		<TextView android:id="@+id/textView1"
			android:layout_width="wrap_content" android:layout_toRightOf="@+id/imageView1"
			android:layout_alignTop="@+id/imageView1" android:layout_alignBottom="@+id/imageView1"
			android:layout_marginLeft="3dip" android:textSize="11sp" android:layout_height="wrap_content" android:width="60dip" android:textColor="@color/white" android:text="Bookmarks" android:layout_marginTop="4dip"></TextView>
		<TableRow android:id="@+id/tableRow3"
			android:background="@color/lightGreen"
			android:layout_width="1dip" android:visibility="visible"
			android:layout_height="fill_parent" android:layout_marginLeft="228dip"></TableRow>
		<Button android:layout_alignParentRight="true" android:layout_centerHorizontal="true" android:layout_marginRight="58dip" android:layout_height="19dip" android:layout_width="19dip" android:layout_marginTop="4dip" android:id="@+id/bookmarkhomeButton" android:background="@drawable/home"></Button>
		<TableRow android:id="@+id/tableRow4"
			android:background="@color/lightGreen"
			android:layout_width="1dip" android:visibility="visible"
			android:layout_height="fill_parent" android:layout_marginLeft="275dip"></TableRow>
		<Button android:background="@drawable/refresh"
			android:layout_alignParentRight="true" android:layout_marginRight="10dip" android:layout_height="19dip" android:layout_width="19dip" android:layout_marginTop="4dip" android:id="@+id/bookmarkrefreshButton"></Button>
	</RelativeLayout>
	<TableRow android:id="@+id/tableRow2" android:layout_width="fill_parent"
		android:background="@color/lightGreen" android:layout_height="1dip"></TableRow>

	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="vertical">
	</LinearLayout>
	
	
	<ScrollView android:id="@+id/scrollView1" android:layout_marginTop="20dip" android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TableLayout android:layout_width="fill_parent" android:id="@+id/bmarkListTable"
		android:layout_height="fill_parent"
		android:orientation="horizontal">
		</TableLayout>
		</ScrollView>
		
	
	
</LinearLayout>