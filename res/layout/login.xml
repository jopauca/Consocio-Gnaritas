<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/widget29"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android" android:background="@color/white">
	<LinearLayout android:id="@+id/widget63"
		android:layout_width="fill_parent" android:orientation="vertical" android:layout_height="200dip">
		<LinearLayout android:layout_height="wrap_content" android:id="@+id/linearLayout1" android:layout_width="fill_parent">
			<ImageView android:layout_gravity="center" android:background="@drawable/logincoag" android:id="@+id/imageView1" android:layout_marginTop="15dip" android:layout_height="120dip" android:layout_width="145dip" android:layout_marginLeft="75dip"></ImageView>
		</LinearLayout>
		<ImageView android:id="@+id/imageView2"
			android:layout_marginRight="50dip" android:background="@drawable/cli" android:layout_width="190dip" android:layout_marginLeft="65dip" android:layout_height="95dip"></ImageView>
	</LinearLayout>
	<LinearLayout android:id="@+id/widget64"
		android:layout_height="wrap_content" android:orientation="vertical"
		android:layout_width="fill_parent" android:layout_marginTop="30dip">
		<EditText android:layout_gravity="center_horizontal"
			android:hint="Username" android:focusable="true" android:imeOptions="actionNext" android:inputType="text" android:layout_width="225dip" android:id="@+id/usernameInput" android:layout_height="45dip">
		</EditText>
		<EditText android:inputType="textPassword" android:imeOptions="actionDone" android:focusable="true" android:layout_gravity="center_horizontal|center"  android:hint="Password" android:layout_width="225dip" android:id="@+id/passwordInput" android:layout_marginTop="5dip" android:layout_height="45dip">
		</EditText>
		<Button android:text="Login" android:focusable="true"
			android:layout_gravity="center_horizontal" android:layout_width="170dip"
			 android:id="@+id/LoginButton" android:layout_marginTop="5dip" android:layout_height="40dip">
		</Button>
	</LinearLayout>
	<LinearLayout android:id="@+id/widget66"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:orientation="vertical">
		<LinearLayout android:id="@+id/widget65"
			android:layout_width="319px" android:layout_height="1px"
			android:orientation="vertical">
		</LinearLayout>
		<CheckBox android:layout_width="wrap_content"
			android:text="Remember Me" android:textSize="14sp" android:textColor="@color/black"
			android:checked="true" android:id="@+id/RememberLogin" android:layout_height="wrap_content">
		</CheckBox>
	</LinearLayout>
	<LinearLayout android:id="@+id/widget73"
		android:layout_width="fill_parent" android:orientation="horizontal" android:layout_gravity="center_horizontal" android:layout_height="50dip">
		<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:textColor="@color/black" android:id="@+id/registerClick" android:layout_gravity="center_horizontal" android:clickable="true" android:layout_marginTop="30dip" android:text="Register &gt;&gt;" android:layout_marginLeft="119dip" android:textSize="14.5sp"></TextView>
	</LinearLayout>
</LinearLayout>