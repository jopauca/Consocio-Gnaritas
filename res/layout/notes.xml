<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="@color/white">
	<RelativeLayout android:id="@+id/widget86"
		android:layout_width="fill_parent" android:layout_height="29dip" android:background="@color/random">
		<ImageView android:id="@+id/imageView1"
			android:layout_alignParentLeft="true" android:layout_width="20dip" android:layout_marginLeft="5dip" android:layout_height="20dip" android:layout_marginTop="4dip" android:background="@drawable/notes"></ImageView>
		<TextView android:id="@+id/textView1"
			android:layout_width="wrap_content" android:layout_toRightOf="@+id/imageView1"
			android:layout_alignTop="@+id/imageView1" android:layout_alignBottom="@+id/imageView1"
			android:layout_marginLeft="3dip" android:textSize="11sp" android:layout_height="wrap_content" android:width="55dip" android:textColor="@color/white" android:layout_marginTop="4dip" android:text="Notes"></TextView>
		<TableRow android:id="@+id/tableRow3"
			android:background="@color/lightGreen"
			android:layout_width="1dip" android:visibility="visible"
			android:layout_height="fill_parent" android:layout_marginLeft="228dip"></TableRow>
		<Button android:layout_alignParentRight="true" android:layout_centerHorizontal="true" android:layout_marginRight="58dip" android:layout_height="19dip" android:layout_width="19dip" android:layout_marginTop="4dip" android:background="@drawable/home" android:id="@+id/noteshomeButton"></Button>
		<TableRow android:id="@+id/tableRow4"
			android:background="@color/lightGreen"
			android:layout_width="1dip" android:visibility="visible"
			android:layout_height="fill_parent" android:layout_marginLeft="275dip"></TableRow>
		<Button android:background="@drawable/refresh"
			android:layout_alignParentRight="true" android:layout_marginRight="10dip" android:layout_height="19dip" android:layout_width="19dip" android:layout_marginTop="4dip" android:id="@+id/notesrefreshButton"></Button>
	</RelativeLayout>
	<LinearLayout android:id="@+id/linearLayout1" android:layout_width="fill_parent" android:orientation="horizontal" android:layout_height="50dip">
		<AutoCompleteTextView android:id="@+id/autoCompleteTextView1" android:layout_height="wrap_content" android:layout_width="fill_parent" android:text="Note Title"></AutoCompleteTextView>
	</LinearLayout>
	<LinearLayout android:id="@+id/linearLayout2" android:layout_width="fill_parent" android:layout_height="200dip">
		<AutoCompleteTextView android:id="@+id/autoCompleteTextView2" android:text="Content" android:layout_height="fill_parent" android:layout_width="fill_parent"></AutoCompleteTextView>
	</LinearLayout>
	<LinearLayout android:id="@+id/linearLayout3" android:layout_height="fill_parent" android:layout_width="fill_parent">
		<Button android:id="@+id/savenoteButton" android:layout_height="40dip" android:layout_marginTop="5dip" android:layout_width="125dip" android:layout_marginLeft="25dip" android:text="Save"></Button>
		<Button android:id="@+id/discardnoteButton" android:layout_height="40dip" android:layout_marginTop="5dip" android:layout_width="125dip" android:layout_marginLeft="20dip" android:text="Discard"></Button>
	</LinearLayout>
</LinearLayout>